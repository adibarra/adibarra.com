    "@vueuse/core": "^13.6.0",
    "dayjs": "^1.11.13",
    "fs-extra": "^11.3.0",
    "gray-matter": "^4.0.3",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-link-attributes": "^4.0.1",
