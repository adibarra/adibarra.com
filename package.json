{
  "name": "adibarra.com",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.15.1",
  "description": "My personal website",
  "contributors": [
    "adibarra <github@adibarra.com> (https://adibarra.com)"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf dist/ auto-imports.d.ts components.d.ts vite.config.ts.timestamp-*",
