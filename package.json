{
  "name": "adibarra.com",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "description": "My portfolio website",
  "contributors": [
    "adibarra <github@adibarra.com> (https://adibarra.com)"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf dist/ auto-imports.d.ts components.d.ts vite.config.ts.timestamp-*",
    "clean:all": "pnpm run clean && rm -rf node_modules/",
    "dev": "vite dev --port 3333 --clearScreen false",
    "lint": "eslint --cache .; exit 0",
    "lint:fix": "eslint --cache . --fix; exit 0",
    "preview": "vite preview --port 3000",
    "taze": "taze -I",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@iconify/json": "^2.2.403",
    "@shikijs/markdown-it": "^3.14.0",
    "@shikijs/twoslash": "^3.14.0",
    "@unhead/vue": "^2.0.19",
    "@unocss/reset": "^66.5.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vueuse/core": "^14.0.0",
    "dayjs": "^1.11.19",
    "fs-extra": "^11.3.2",
    "gray-matter": "^4.0.3",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-table-of-contents": "^1.1.0",
    "nprogress": "^0.2.0",
    "shiki": "^3.14.0",
    "unocss": "^66.5.4",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
